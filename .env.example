# Copy to .env and fill in values.
PARALLEL_API_KEY=your_parallel_api_key_here
DATAGEN_API_KEY=your_datagen_api_key_here

# DataGen MCP Server (for Claude Agent SDK)
# Agents will have access to DataGen MCP tools (Slack, Gmail, Supabase, etc.)
# DATAGEN_MCP_URL=https://mcp.datagen.dev/mcp  # (default, can override)

# Optional overrides:
PARALLEL_BASE_URL=https://api.parallel.ai
PARALLEL_BETA_HEADER=findall-2025-09-15
FINDALL_GENERATOR=core
FINDALL_MATCH_LIMIT=25
FINDALL_MAX_WORKERS=3
FINDALL_POLL_INTERVAL=2.0
FINDALL_POLL_TIMEOUT=600.0
